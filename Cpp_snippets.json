{
  "File-Header": {
    "prefix": "file-header",
    "body": [
      "// ---------------------------------------------------------------------------------------------------------------------\n// filename:    ${1:<filename>}\n//\n// Description: ${2:<description>}\n//\n// Group:        ${3:<group>}\n// ---------------------------------------------------------------------------------------------------------------------"
    ]
  },
  "Large-Function-Header": {
    "prefix": "func-header",
    "body": [
      "// ---------------------------------------------------------------------------------------------------------------------\n// ${1:<Description>}\n//\n// @param: ${2:<param1>}\n// @param: ${3:<param2>}\n//\n// @return: ${4:<return>}\n//"
    ]
  },
  "Small-Function-Header": {
    "prefix": "c-header",
    "body": [
      "// ---------------------------------------------------------------------------------------------------------------------\n// ${1:<Description>}\n//"
    ]
  },
  "Section-Comment": {
    "prefix": "comment-section",
    "body": [
      "// ---------------------------------------------------------------------------------------------------------------------\n//                          ${1:<Section>}\n// ---------------------------------------------------------------------------------------------------------------------"
    ]
  }
}