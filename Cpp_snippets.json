{
  "File-Header": {
    "prefix": "file-header",
    "body": [
      "// ---------------------------------------------------------------------------------------------------------------------\n// filename:    ${1:<filename>}\n//\n// Description: ${2:<description>}\n//\n//\n// SudentID:    ${4:name}| ${5:studentID}\n// ---------------------------------------------------------------------------------------------------------------------"
    ],
  },
  "Function-Header": {
    "prefix": "function-header",
    "body": [
      "// ---------------------------------------------------------------------------------------------------------------------\n// ${1:<Description>}\n//\n// @param: ${2:<param1>}\n// @param: ${3:<param2>}\n//\n// @return: ${4:<return>}\n//"
    ]
  },
  "Section-Comment": {
    "prefix": "comment-section",
    "body": [
      "// ---------------------------------------------------------------------------------------------------------------------\n//                          ${1:<Section>}\n// ---------------------------------------------------------------------------------------------------------------------"
    ]
  }
}